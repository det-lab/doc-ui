<header class="header">
  <nav class="navbar" id="topbar">
    <div class="container">
      <div class="navbar-brand">
        <a class="navbar-item" href="https://www.couchbase.com"><img src="{{uiRootPath}}/img/logo.svg" alt="Couchbase"></a>
        <button class="navbar-burger" data-target="topbar-menu">
          <span></span>
          <span></span>
          <span></span>
        </button>
      </div>
      <div id="topbar-menu" class="navbar-menu">
        <div class="navbar-start">
          <div class="navbar-item has-dropdown">
            <a class="navbar-link" href="{{or site.url (or siteRootUrl siteRootPath)}}">Docs</a>
            <div class="navbar-dropdown explore">
              <div class="title">Det-Lab Documentation Overview</div>
              <div class="cols">
                <ul>
                  <li class="heading"><a href="{{siteRootPath}}/server/6.0/introduction/intro.html">Physics</a></li>
                  <li><a href="{{siteRootPath}}/server/6.0/n1ql/n1ql-language-reference/index.html">Dark Matter</a></li>
                  <li><a href="{{siteRootPath}}/server/6.0/fts/full-text-intro.html">CDMS Detectors</a></li>
                  <li><a href="{{siteRootPath}}/server/6.0/analytics/introduction.html">Phonon Sensors</a></li>
                  <li><a href="{{siteRootPath}}/server/6.0/eventing/eventing-overview.html">Radiation</a></li>
                  <li><a href="{{{relativize site.components.operator.url}}}">Autonomous Operator</a></li>
                </ul>
                <ul>
                  <li class="heading">Linux Environment</li>
                  <li><a href="{{{relativize site.components.couchbase-lite.url}}}">Common Errors</a></li>
                  <li><a href="{{{relativize site.components.sync-gateway.url}}}">Shell Basics</a></li>
                </ul>
                <ul class="two-cols">
                  <li class="heading"><a href="{{siteRootPath}}/server/6.0/sdk/overview.html">Code</a></li>
                  <li><a href="{{siteRootPath}}/c-sdk/2.10/start-using-sdk.html">Git</a></li>
                  <li><a href="{{siteRootPath}}/dotnet-sdk/2.7/start-using-sdk.html">Testing</a></li>
                  <li><a href="{{siteRootPath}}/go-sdk/1.5/start-using-sdk.html">Best Practices</a></li>
                </ul>
                                <ul>
                  <li class="heading">Writing</li>
                  <li><a href="{{{relativize site.components.couchbase-lite.url}}}">Adding to this Documentation</a></li>
                  <li><a href="{{{relativize site.components.sync-gateway.url}}}">Analaysis Notes</a></li>
                  <li><a href="{{{relativize site.components.sync-gateway.url}}}">Papers and Theses</a></li>
                </ul>
              </div>
            </div>
          </div>
          {{#if (and page.component (ne page.component.name 'home'))}}
          {{#if (or page.versions (ends-with page.component.name '-sdk'))}}
          <div class="navbar-item has-dropdown">
            <a class="navbar-link component" href="{{{relativize page.componentVersion.url}}}"><span class="title">{{{page.component.title}}}</span> <span class="version">{{page.componentVersion.displayVersion}}</span></a>
            <div class="navbar-dropdown versions">
              <div class="cols">
                <ul>
                  {{#each page.versions}}
                  {{#unless (eq ./version @root.page.version)}}
                  <li><a class="navbar-item" href="{{{relativize ./url}}}">{{{@root.page.component.title}}} {{./displayVersion}}</a></li>
                  {{/unless}}
                  {{/each}}
                </ul>
                {{#if (ends-with page.component.name '-sdk')}}
                <ul class="related">
                  {{#each (related-sdk-pages page.component.name page.url 'c-sdk/2.10,dotnet-sdk/2.7,go-sdk/1.5,java-sdk/2.7,nodejs-sdk/2.6,php-sdk/2.6,python-sdk/2.5')}}
                  <li><a class="navbar-item" href="{{{relativize ./url}}}">{{./title}}</a></li>
                  {{/each}}
                </ul>
                {{/if}}
              </div>
            </div>
          </div>
          {{else}}
          <a class="navbar-item component" href="{{{relativize page.componentVersion.url}}}"><span class="title">{{{page.component.title}}}</span>{{#unless (eq page.componentVersion.displayVersion 'master')}} <span class="version">{{page.componentVersion.displayVersion}}</span>{{/unless}}</a>
          {{/if}}
          {{/if}}
          {{#if env.ALGOLIA_API_KEY}}
          <form class="navbar-item search">
            <input type="text" placeholder="Search Det-Lab Docs" class="query" id="search-query">
            <button type="button" class="search"><span>Search</span></button>
          </form>
          {{/if}}
        </div>
        <div class="navbar-end">
          <div class="navbar-item">
            <a class="btn red-btn" href="https://github.com/det-lab">Github</a>
          </div>
        </div>
      </div>
    </div>
  </nav>
</header>
